<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.mredrock.cyxbs.freshman.view.widget.SuccessView
        android:layout_width="53dp"
        android:layout_height="53dp"
        app:circleDuration="500"
        app:gouDuration="600"
        app:lineWidth="1dp"
        android:id="@+id/success_view_copy_qq_number_success"
        app:color="@color/freshman_copy_qq_number_success_gou_color"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="37dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/success_view_copy_qq_number_success"
        android:layout_marginTop="15dp"
        android:textColor="@color/freshman_copy_qq_number_success_text_color"
        android:text="@string/freshman_copy_qq_number_success"/>

</RelativeLayout>